<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>
  <% include ../partials/nav.ejs %>
<div class="jumbotron text-center">
  <div class="container">
    <h1>WeMusic</h1>
    <p><i>The songs we like together.</i></p>
  </div>
</div>
<div class="container">
  <div class="alert alert-info text-center" role="alert">
   UNDER CONSTRUCTION
 </div>
  <hr>
  <div class="row">
    <div class="col-md-6">
      <h3>The Music List</h3>
      <ul>
          <% results.forEach(function(r) { %>
              <li><%= r.song %> - <%= r.artist %></li>
          <% }); %>
      </ul>
    </div>
    <div class="col-md-6">
      <h3> Add your song</h3>
        <form  method="post" action="add">
            <input type="text" name="song">
              <input type="text" name="artist">
              <input type="submit" name="Submit" value="Insert" class="btn">
            </form>
    </div>
  </div>
   <div class="alert alert-info text-center" role="alert">
    UNDER CONSTRUCTION
  </div>
</div>
</body>
</html>
